/*************************************************************
 *
 *   LoginModule: username:	password:	bad password:
 *
 *   JCKLoginMod1	Pooh		honey		money
 *   JCKLoginMod2	Eyeore	tail		taiL
 *   JCKLoginMod3	Tigger	bounce	BOUNCE
 *   JCKLoginMod4	Piglet	pink		punk
 *   JCKLoginMod5	Roo		joey		joel
 *   JCKLoginMod6	Rabbit	carrot	CarRot
 *
 *   Add 'debug=true' option to display debugging comments.
 *
 *************************************************************/

loginGood1 {
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod1 required username="Pooh" password="honey";
};

loginGood2 {
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod1 required username="Pooh" password="honey";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod2 requisite username="Eyeore" password="tail";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod3 sufficient username="Tigger" password="bounce";
};

// JCKLoginMod2 fails but it is only 'optional'
loginGood3 {
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod5 required username="Roo" password="joey";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod2 optional username="Eyeore" password="taiL";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod3 sufficient username="Tigger" password="bounce";
};

// JCKLoginMod6 fails but authentication continues to JCKLoginMod4.
// JCKLoginMod1 fails but it is only 'optional'.
loginGood4 {
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod5 required username="Roo" password="joey";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod2 requisite username="Eyeore" password="tail";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod3 required username="Tigger" password="bounce";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod6 sufficient username="Rabbit" password="CarRot";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod4 sufficient username="Piglet" password="pink";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod1 optional username="Pooh" password="money";
};

//  These fail.

//  'required' JCKLoginMod1 fails.
loginBad1 {
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod1 required username="Pooh" password="money";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod4 required username="Piglet" password="pink";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod6 required username="Rabbit" password="carrot";
};

//  'requisite' JCKLoginMod4 fails.  
//  'optional' JCKLoginMod6 fails too, but it should never get invoked.
loginBad2 {
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod2 required username="Eyeore" password="tail";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod4 requisite username="Piglet" password="punk";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod6 optional username="Rabbit" password="CarRot";
};

//  'requisite' JCKLoginMod4 fails.
//  'optional' JCKLoginMod6 fails too, but it should never get invoked.
loginBad3 {
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod1 required username="Pooh" password="honey";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod2 requisite username="Eyeore" password="tail";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod3 required username="Tigger" password="bounce";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod4 requisite username="piglet" password="punk";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod6 optional username="Rabbit" password="CarRot";
};

//  'optional' JCKLoginMod6 fails (ok).
//  Then 'required' JCKLoginMod1 fails.
loginBad4 {
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod2 required username="Eyeore" password="tail";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod4 requisite username="Piglet" password="punk";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod6 optional username="Rabbit" password="CarRot";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod5 required username="Roo" password="joey";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod3 requisite username="Tigger" password="bounce";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod1 required username="Pooh" password="money";
};

//  No modules succeed.
loginBad5 {
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod1 sufficient username="Pooh" password="money";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod2 sufficient username="Eyeore" password="taiL";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod3 optional username="Tigger" password="BOUNCE";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod4 sufficient username="piglet" password="punk";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod5 optional username="Rooooo" password="joey";
   javasoft.sqe.jck.lib.security.auth.module.JCKLoginMod6 optional username="Rabbit" password="CarRot";
};

traceSuccessLogin {
   javasoft.sqe.jck.lib.security.auth.module.trace.TraceLoginModule required id="passed" status.login="true";
   javasoft.sqe.jck.lib.security.auth.module.trace.TraceLoginModule optional id="failed" status.login="false";
};

traceFailedLogin {
   javasoft.sqe.jck.lib.security.auth.module.trace.TraceLoginModule required id="passed" status.login="true";
   javasoft.sqe.jck.lib.security.auth.module.trace.TraceLoginModule optional id="failedOptional" status.login="false";
   javasoft.sqe.jck.lib.security.auth.module.trace.TraceLoginModule required id="failedRequired" status.login="false";
};

traceFailedCommit {
   javasoft.sqe.jck.lib.security.auth.module.trace.TraceLoginModule required  id="passed" status.login="true";
   javasoft.sqe.jck.lib.security.auth.module.trace.TraceLoginModule optional  id="passedOptional" status.login="true";
   javasoft.sqe.jck.lib.security.auth.module.trace.TraceLoginModule requisite id="passedRequisite" status.login="true";
   javasoft.sqe.jck.lib.security.auth.module.trace.TraceLoginModule required  id="failedCommit" status.login="true" status.commit="false";
};

traceFailedAbort {
   javasoft.sqe.jck.lib.security.auth.module.trace.TraceLoginModule required id="firstFailed"  status.login="false" status.abort="false";
   javasoft.sqe.jck.lib.security.auth.module.trace.TraceLoginModule optional id="secondFailed" status.login="false" status.abort="false";
   javasoft.sqe.jck.lib.security.auth.module.trace.TraceLoginModule required id="thirdFailed"  status.login="false" status.abort="false";
};



/**
 * Login Configuration for the Kerberos tests.
 */

SampleClient {
  com.sun.security.auth.module.Krb5LoginModule required refreshKrb5Config=true;
};

SampleServer {
  com.sun.security.auth.module.Krb5LoginModule required storeKey=true refreshKrb5Config=true;
};


