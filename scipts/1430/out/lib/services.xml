<services>

    <service id="RMI_service" class="com.sun.jck.services.RMIService" description="The service starts rmid"/>
    <service id="ORB_service" class="com.sun.jck.services.ORBService" description="The service starts tnamesev"/>
    <service id="Distributed_Agent" class="com.sun.jck.services.DistributedAgentService" description="The service starts a remote agent for distributed tests"/>
    <service id="Agent" class="com.sun.jck.services.AgentService" description="The service starts an agent for regular test run"/>

    <testpath path="rmid">
        <service refid="RMI_service"/>
    </testpath>

    <testpath path="tnameserv">
        <service refid="ORB_service"/>
    </testpath>

    <testpath path="distributed">
        <service refid="Distributed_Agent"/>
    </testpath>

    <testpath path="">
        <service refid="Agent"/>
    </testpath>
</services>
